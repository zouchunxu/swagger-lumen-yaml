{
    "name": "yunhantech/swagger-lumen-yaml",
    "type": "library",
    "description": "Swagger文档，用PHP注释可读性差，维护性差，使用YAML格式",
    "keywords": [
        "Lumen",
        "YunhanTech",
        "Swagger",
        "YAML"
    ],
    "homepage": "https://github.com/YunhanTech/swagger-lumen-yaml",
    "license": "MIT",
    "authors": [
        {
            "name": "RayGu",
            "email": "raygu0101@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.1.3",
        "laravel/lumen-framework": "^5.5"
    },
    "require-dev": {
        "darkaonline/swagger-lume": "^5.5",
        "symfony/yaml": "^4.1",
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "7.0.*",
        "mockery/mockery": "~1.0",
        "satooshi/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "YunhanTech\\Swagger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "prefer-stable": true
}
